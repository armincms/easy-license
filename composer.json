{
    "name": "armincms/easy-license",
    "description": "A Laravel Nova tool.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": ">=8.0",
        "illuminate/support": "~9.0",
        "armincms/contract": "^2.0",
        "armincms/categorizable": "^3.0",
        "armincms/nova-targomaan": "^2.0",
        "armincms/duration": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Armincms\\EasyLicense\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Armincms\\EasyLicense\\ServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "version": "2.0.0"
}
